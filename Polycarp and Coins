#include<bits/stdc++.h>
#define ll long long int

using namespace std;

void solve()
{
    ll n;cin >> n;

    ll c1,c2;

    c1 = n/3;
    c2 = n/3;

    if(n%3 == 0)
        cout << c1 << " " << c2 << endl;
    else if((c1+1) + (c2*2) == n)
        cout << c1+1 << " " << c2 << endl;
    else
        cout << c1 << " " << c2+1 << endl;
}

int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);

    int t;
    cin >> t;

    while(t--)
    {
        solve();
    }

    return 0;
}

